{
  "name": "react-native-elements-app",
  "version": "1.0.0",
  "description": "StockedgeClub",
  "author": "Monte Thakkar",
  "private": true,
  "homepage": "https://stockedgeclub.com",
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "expoex start --web",
    "test": "jest --watchAll",
    "build:web": "expo build:web --config ./app.web-build.json --polyfill",
    "deploy": "gh-pages -d web-build",
    "prettify": "prettier --write 'src/**/*.js'",
    "clean-install": "rm -rf node_modules && npm cache clean --force && watchman watch-del-all && yarn",
    "postinstall": "jetify",
    "preandroid": "npx jetify"
  },
  "dependencies": {
    "@expo/vector-icons": "^10.0.6",
    "expo": "^34.0.1",
    "expo-asset": "^6.0.0",
    "expo-font": "~6.0.0",
    "jetifier": "^1.6.5",
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "react": "16.8.3",
    "react-dom": "^16.8.6",
    "react-native": "0.59.10",
    "react-native-elements": "^1.2.5",
    "react-native-gesture-handler": "~1.3.0",
    "react-native-reanimated": "~1.1.0",
    "react-native-screens": "1.0.0-alpha.22",
    "react-native-touchable-scale": "^2.0.0",
    "react-native-unimodules": "~0.5.2",
    "react-native-web": "^0.11.4",
    "react-navigation": "^4.0.10",
    "react-navigation-drawer": "^1.4.0",
    "react-navigation-stack": "^1.7.3",
    "react-navigation-tabs": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@expo/webpack-config": "^0.7.3",
    "babel-jest": "24.1.0",
    "babel-preset-expo": "^6.0.0",
    "gh-pages": "^2.1.1",
    "jest": "24.1.0",
    "jest-expo": "^34.0.0",
    "metro-react-native-babel-preset": "0.54.0",
    "react-test-renderer": "16.8.3"
  },
  "jest": {
    "preset": "jest-expo"
  }
}
